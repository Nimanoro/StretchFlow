{
  "expo": {
    "name": "StretchFlow",
    "slug": "StretchFlow",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/hero.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/hero.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.nimanorouzi.stretchflow",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
    "NSHealthShareUsageDescription": "StretchFlow uses Health data to log mindfulness minutes and help you stay consistent.",
    "NSHealthUpdateUsageDescription": "StretchFlow writes mindfulness data to your Health app to help track your progress."
    }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nimanorouzi.stretchflow"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7594575a-742f-4157-8eb5-61dc3a0ed57b"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7594575a-742f-4157-8eb5-61dc3a0ed57b",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
